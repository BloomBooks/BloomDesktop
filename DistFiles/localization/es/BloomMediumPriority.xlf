        <note>ID: EditTab.Toolbox.TalkingBookTool.EditTimingsFileTip</note>
      </trans-unit>
      <trans-unit id="EditTab.Toolbox.TalkingBookTool.EditTimingsFileDisabledTip" sil:dynamic="true">
