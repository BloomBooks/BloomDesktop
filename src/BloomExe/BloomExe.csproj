	</Target>

	<Target Name="CopyPlatformSpecificFiles" AfterTargets="CopySomeFiles">
		<Copy SourceFiles="@(FfmpegFiles)" DestinationFolder="$(OutDir)" />
		<Message Text="Copying platform-specific ffmpeg for $(Platform)" Importance="high" />

		<Error Text="Platform-specific ffmpeg not found for $(Platform) - expected at @(FfmpegFiles)"
		       Condition="'$(Platform)'=='ARM64' AND !Exists('@(FfmpegFiles)')" />
	</Target>
	<Target Name="ReallyAfterBuild" AfterTargets="CopySomeFiles">
		<Copy SourceFiles="@(CopyForSILMedia)" DestinationFolder="$(OutDir)" />
		<Delete Files="@(DeleteForSILMedia)" TreatErrorsAsWarnings="true" />
		<RemoveDir Directories="@(RemoveForSILMedia)" />
		<Message Text="Copying Palaso.xlf files from nuget package to DistFiles" Importance="high" />
		<Copy SourceFiles="@(SourcePalasoFile)" DestinationFiles="$(LocalizationDir)en\Palaso.xlf" />
		<Copy SourceFiles="@(TranslatedPalasoFiles)" DestinationFiles="$(LocalizationDir)%(RecursiveDir)Palaso.xlf" Condition="'%(RecursiveDir)' != ''" />
		<Message Text="ReallyAfterBuild target completed." Importance="high" />
	</Target>
	<!--
		The SIL Common Strings project uses pt-BR as the tag for Portuguese.  SIL-Bloom uses
		pt-PT as the tag.  This needs to be fixed for Bloom to use the localizations from
		libpalaso.  Using the FileUpdate task from the SIL.BuildTasks package seems like the
		easiest way to achieve this result.
	-->
	<UsingTask TaskName="FileUpdate" AssemblyFile="$(PkgSIL_BuildTasks)\tools\SIL.BuildTasks.dll" />
	<Target Name="FixPortuguese" AfterTargets="ReallyAfterBuild">
		<FileUpdate File="$(LocalizationDir)\pt\Palaso.xlf" Regex="pt-BR" ReplacementText="pt-PT" />
		<Message Text="Finished fixing Portuguese language tags in Palaso.xlf" Importance="high" />
	</Target>

	<!-- ARM64 SQLite support - fail fast if dependency not properly restored -->
	<Target Name="CopyArm64Sqlite" AfterTargets="ReallyAfterBuild" Condition="'$(Platform)'=='ARM64'">
		<PropertyGroup>
			<Arm64SqliteSource>$(OutDir)runtimes\win-arm64\native\e_sqlite3.dll</Arm64SqliteSource>
			<Arm64SqliteTarget>$(OutDir)e_sqlite3.dll</Arm64SqliteTarget>
		</PropertyGroup>
		
		<!-- Fail immediately if ARM64 SQLite library is not where NuGet should have placed it -->
		<Error Text="ARM64 SQLite library not found at expected location: $(Arm64SqliteSource). Check that RuntimeIdentifiers includes 'win-arm64' and NuGet restore completed successfully."
		       Condition="!Exists('$(Arm64SqliteSource)')" />
		
		<!-- Copy ARM64 SQLite library to main output directory -->
		<Copy SourceFiles="$(Arm64SqliteSource)" DestinationFiles="$(Arm64SqliteTarget)" />
		<Message Text="Copied ARM64 SQLite library: $(Arm64SqliteSource) -> $(Arm64SqliteTarget)" Importance="high" />
	</Target>
