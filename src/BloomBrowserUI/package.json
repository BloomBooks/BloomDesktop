{
    "name": "bloom",
    "version": "5.3.0",
    "description": "Bloom Editor",
    "main": "none",
    "sideEffects": false,
    "directories": {
        "test": "test"
    },
    "engines": {
        "node": "~16.13.1"
    },
    "scripts": {
        "test": "karma start --browsers Firefox  --single-run",
        "testWatch": "karma start --browsers Firefox",
        "testWatchChrome": "karma start --browsers Chrome",
        "teamcitytest": "karma start karma.conf.js --browsers Chrome --reporters teamcity --single-run",
        "// 'build': 'does a nice new clean build of the output/browser folder.'": "",
        "build": "gulp && npm run build:content",
        "build:pug": "gulp pug",
        "build-short": "gulp build-short && npm run build:content",
        "build-prod": "gulp build-prod  && npm run build:content",
        "// 'build-translate': creates/updates xliff files and translates html files.": "",
        "// 'build-translate': is needed when markdown/html content changes or when testing l10n.": "",
        "// 'build-translate': should be run after build.  (build-prod includes this functionality.)": "",
        "build-translate": "gulp build-translate",
        "build:content": "cd ../content && npm run build",
        "watchCode": "set NODE_ENV=debug&& webpack --watch",
        "watchBookEditLess": "less-watch-compiler bookEdit ../../output/browser/bookEdit",
        "watchPageChooserLess": "less-watch-compiler pageChooser ../../output/browser/pageChooser",
        "watchProblemDialogLess": "less-watch-compiler problemDialog ../../output/browser/problemDialog",
        "watch": "npm-run-all --parallel watchBookEditLess watchPageChooserLess watchProblemDialogLess watchCode",
        "gitSemiClean": "git clean -X --dry-run --exclude='!node_modules'",
        "gitSemiCleanForReal": "git clean -fX --exclude='!node_modules'",
        "tslint": "tslint --project tsconfig.json",
        "// use output/browser as the root of the storybook-provided server": "",
        "// specify the port (-p 58886) so we can have special logic in bloomApi.ts for storybook": "",
        "storybook": "start-storybook -p 58886 -s ../../output/browser --static-dir ./,./react_components,./teamCollection",
        "storybook-cors": "npm-run-all --parallel storybook unsafe-chrome",
        "unsafe-chrome": "chrome http://localhost:58886/ --disable-web-security --disable-gpu --user-data-dir=%temp%/chromeTemp",
        "postinstall": "patch-package"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/BloomBooks"
    },
